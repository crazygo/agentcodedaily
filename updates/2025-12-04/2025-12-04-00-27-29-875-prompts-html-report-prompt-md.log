er-left: 3px solid #ff6600;\n            padding: 8px 10px;\n            margin: 8px 0;\n            font-style: italic;\n            font-size: 11px;\n            color: #666666;\n        }\n\n        .feedback-source {\n            display: block;\n            text-align: right;\n            font-size: 10px;\n            color: #828282;\n            margin-top: 4px;\n            font-style: normal;\n        }\n\n        /* Empty State */\n        .empty-state {\n            text-align: center;\n            color: #828282;\n            font-style: italic;\n            padding: 20px;\n            font-size: 12px;\n        }\n\n        /* Footer */\n        footer {\n            background-color: #f6f6ef;\n            border-top: 1px solid #d4d4d4;\n            padding: 15px 0;\n            text-align: center;\n            color: #828282;\n            font-size: 11px;\n            margin-top: 30px;\n        }\n\n        footer a {\n            color: #828282;\n            text-decoration: none;\n        }\n\n        footer a:hover {\n            text-decoration: underline;\n        }\n\n        /* Responsive Design */\n        @media (max-width: 968px) {\n            .three-column-layout {\n                grid-template-columns: 1fr;\n                gap: 15px;\n            }\n        }\n\n        @media (max-width: 600px) {\n            .header-content {\n                flex-direction: column;\n                text-align: center;\n                gap: 5px;\n            }\n\n            nav {\n                text-align: center;\n            }\n\n            nav a {\n                display: block;\n                margin: 5px 0;\n            }\n\n            .container {\n                padding: 0 5px;\n            }\n\n            section {\n                padding: 10px;\n            }\n        }\n\n        /* Print Styles */\n        @media print {\n            body {\n                background-color: #ffffff;\n                color: #000000;\n            }\n\